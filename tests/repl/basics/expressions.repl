#@ session: basic_expressions
#@ description: Test basic expression evaluation and auto-printing

# Integer literals
>>> 42
=> 42

>>> -17
=> -17

>>> 0
=> 0

# Float literals
>>> 3.14
=> 3.14

>>> -2.5
=> -2.5

# Arithmetic operations
>>> 2 + 3
=> 5

>>> 10 - 4
=> 6

>>> 6 * 7
=> 42

>>> 20 / 4
=> 5

>>> 17 % 5
=> 2

# Operator precedence
>>> 2 + 3 * 4
=> 14

>>> (2 + 3) * 4
=> 20

# Boolean literals
>>> true
=> true

>>> false
=> false

# Comparison operators
>>> 5 > 3
=> true

>>> 5 < 3
=> false

>>> 5 == 5
=> true

>>> 5 != 3
=> true

>>> 5 >= 5
=> true

>>> 5 <= 4
=> false

# Logical operators
>>> true && false
=> false

>>> true || false
=> true

>>> !true
=> false

>>> !false
=> true

# Negative numbers
>>> -5 * 3
=> -15

>>> -10 + 3
=> -7

# Floating point arithmetic
>>> 1.5 + 2.5
=> 4

>>> 3.0 * 2.0
=> 6
