#@ session: maps
#@ description: Test map literals and methods

# Map access
>>> m = {"a" => 1, "b" => 2}
=> map[string]int{"a":1, "b":2}

>>> m["a"]
=> 1

>>> m["b"]
=> 2

# Map length
>>> m.length
=> 2

# Map contains key
>>> m.has_key?("a")
=> true

>>> m.has_key?("x")
=> false

# Map with default
>>> m.fetch("a", 0)
=> 1

>>> m.fetch("x", 0)
=> 0
