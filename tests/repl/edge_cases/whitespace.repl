#@ session: whitespace
#@ description: Test whitespace handling in various contexts

# Whitespace around operators
>>> 2+3
=> 5

>>> 2 + 3
=> 5

>>> 2  +  3
=> 5

# Whitespace in array literals (Go format)
>>> [1,2,3]
=> [1 2 3]

>>> [ 1, 2, 3 ]
=> [1 2 3]

>>> [1 , 2 , 3]
=> [1 2 3]

# Whitespace in function calls
>>> def foo(a: Int, b: Int): Int
...   a + b
... end
=> (defined)

>>> foo(1,2)
=> 3

>>> foo(1, 2)
=> 3

>>> foo( 1 , 2 )
=> 3
