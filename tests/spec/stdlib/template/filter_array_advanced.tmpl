{# data: {
  "numbers": [3, 1, 4, 1, 5, 9, 2, 6],
  "words": ["banana", "Apple", "cherry"],
  "items": [
    {"name": "a", "price": 30, "active": true},
    {"name": "b", "price": 10, "active": false},
    {"name": "c", "price": 20, "active": true}
  ],
  "arr1": [1, 2, 3],
  "arr2": [3, 4, 5],
  "with_nil": [1, null, 2, null, 3],
  "prices": [10.5, 20.0, 15.5]
} #}
sort numbers: {{ numbers | sort | join: ", " }}
sort words: {{ words | sort | join: ", " }}
sort_natural: {{ words | sort_natural | join: ", " }}
sort by prop: {{ items | sort: "price" | map: "name" | join: ", " }}
uniq: {{ numbers | uniq | join: ", " }}
compact: {{ with_nil | compact | join: ", " }}
concat: {{ arr1 | concat: arr2 | join: ", " }}
map: {{ items | map: "name" | join: ", " }}
where active: {{ items | where: "active", true | map: "name" | join: ", " }}
where inactive: {{ items | where: "active", false | map: "name" | join: ", " }}
{% assign found = items | find: "name", "b" %}
find: {{ found.price }}
sum: {{ prices | sum }}
sum by prop: {{ items | sum: "price" }}
